{
  "name": "pump",
  "version": "1.2.2",
  "description": "Pump backend",
  "scripts": {
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "sls:offline": "sls offline start --stage local",
    "sls:deploy:dev": "sls deploy --stage dev",
    "sls:deploy:prod": "sls deploy --stage prod",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepare": "husky install",
    "commit": "git add . && git-cz",
    "release": "standard-version"
  },
  "engines": {
    "node": "16.15.1"
  },
  "dependencies": {
    "@middy/core": "^3.4.0",
    "@middy/http-json-body-parser": "^3.4.0",
    "aws-sdk": "^2.1286.0",
    "axios": "^1.2.2",
    "node-fetch": "^3.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@aws-sdk/util-dynamodb": "^3.245.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/chai": "^4.3.4",
    "@types/jest": "^29.2.5",
    "@types/node": "^14.14.25",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "chai": "^4.3.7",
    "commitizen": "^4.2.6",
    "commitlint": "^17.4.2",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.31.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-html-reporters": "^3.0.11",
    "jest-sonar-reporter": "^2.0.0",
    "json-schema-to-ts": "^1.5.0",
    "prettier": "^2.8.1",
    "serverless": "^3.0.0",
    "serverless-auto-swagger": "^2.12.0",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-esbuild": "^1.37.0",
    "serverless-offline": "^12.0.3",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.9.4"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
